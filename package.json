{
  "name": "metrix-css",
  "version": "0.0.1",
  "description": "style for element in html",
  "main": "index.js",
  "watch": {
    "scss": {
      "extensions": "scss",
      "quiet": false,
      "patterns": [
        "scss"
      ]
    },
    "pug": {
      "extensions": "pug",
      "quiet": false,
      "patterns": [
        "./"
      ]
    }
  },
  "scripts": {
    "scss": "node ./scss-build.js",
    "pug": "node ./pug-build.js",
    "watch": "npm-watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nmhillusion/metrix-css.git"
  },
  "keywords": [
    "nmhillusion"
  ],
  "author": "nmhillusion",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nmhillusion/metrix-css/issues"
  },
  "homepage": "https://github.com/nmhillusion/metrix-css#readme",
  "dependencies": {
    "node-sass": "^4.12.0",
    "npm-watch": "^0.6.0",
    "pug": "^2.0.4"
  }
}
